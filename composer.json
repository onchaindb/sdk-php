{
    "name": "onchaindb/sdk",
    "description": "PHP SDK for OnChainDB - Decentralized database built on Celestia",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "OnChainDB Team",
            "email": "team@onchaindb.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "OnChainDB\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OnChainDB\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
